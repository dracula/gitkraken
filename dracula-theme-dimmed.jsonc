{
    "meta": {
        "name": "Dracula - Dimmed",
        "scheme": "dark" // must be "light" or "dark"
    },
    "themeValues": {
        // values applied to the entire app
        "root": {
            "red": "#FF5555",
            "orange": "#FFB86C",
            "yellow": "#F1FA8C",
            "green": "#50FA7B",
            "teal": "#8BE9FD",
            "blue": "#BD93F9",
            "ltblue": "#8BE9FD",
            "purple": "#FF79C6",
            "app__bg0": "#2E3440",
            "toolbar__bg0": "#3B4252",
            "toolbar__bg1": "#3B4252",
            "toolbar__bg2": "#3B4252",
            "panel__bg0": "#2E3440",
            "panel__bg1": "#3B4252",
            "panel__bg2": "#3B4252",
            "input__bg": "#3B4252",
            "input-bg-warn-color": "#FFB86C",
            "panel-border": "#3B4252",
            "section-border": "#3B4252",
            "subtle-border": "#3B4252",
            "modal-overlay-color": "rgba(0,0,0,.5)",
            // graph colors
            "graph-color-0": "#8BE9FD",
            "graph-color-1": "#50FA7B",
            "graph-color-2": "#FF79C6",
            "graph-color-3": "#BD93F9",
            "graph-color-4": "#F1FA8C",
            "graph-color-5": "#FFB86C",
            "graph-color-6": "#FF5555",
            "graph-color-7": "#8BE9FD",
            "graph-color-8": "#50FA7B",
            "graph-color-9": "#FF79C6",
            "stats-added-color": "#50FA7B",
            "stats-deleted-color": "#FF5555",
            "stats-files-color": "#F1FA8C",
            // text colors
            // values starting with . aren't added to the CSS, they're just variables
            ".text-color": "#D8DEE9",
            "text-selected": "@.text-color",
            "text-normal": "rgba(216,222,233,.8)",
            "text-secondary": "rgba(216,222,233,.6)",
            "text-disabled": "rgba(216,222,233,.4)",
            "text-accent": "#BD93F9",
            "text-inverse": "#222222",
            "text-bright": "@.text-color",
            "text-dimmed": "rgba(216,222,233,.2)",
            "text-dimmed-selected": "rgba(216,222,233,.5)",
            // buttons
            "text-selected-row": "@text-selected",
            "btn-text": "@text-normal",
            "btn-text-hover": "@text-selected",
            "default-border": "@text-normal",
            "default-bg": "transparent",
            "default-hover": "transparent",
            "default-border-hover": "@text-selected",
            "primary-border": "#BD93F9",
            "primary-bg": "rgba(189,147,249,.1)",
            "primary-hover": "rgba(189,147,249,.3)",
