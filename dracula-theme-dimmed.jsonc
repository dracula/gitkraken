        // override specific values just for the toolbar
        "toolbar": {
            "text-selected": "rgba(216,222,233,1)",
            "text-normal": "rgba(216,222,233,.9)",
            "text-secondary": "rgba(216,222,233,.6)",
            "text-disabled": "rgba(216,222,233,.4)",
            "section-border": "rgba(216,222,233,.2)",
            "input__bg": "#3B4252",
            "link-color": "#8BE9FD",
            "btn-text": "var(--text-normal)"
        },
        // override specific values just for the tabs bar
        "tabsbar": {
            "text-selected": "rgba(216,222,233,1)",
            "text-normal": "rgba(216,222,233,.9)",
            "text-secondary": "rgba(216,222,233,.6)",
            "text-disabled": "rgba(216,222,233,.4)",
            "section-border": "rgba(216,222,233,.2)",
            "input__bg": "#3B4252",
            "link-color": "#8BE9FD",
            "btn-text": "var(--text-normal)"
        }
